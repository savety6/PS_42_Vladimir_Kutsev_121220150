<Page x:Class="UI.Windows.ErrorLogPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UI.Windows"
      xmlns:local2="clr-namespace:UI.Components"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ErrorLogPage">

    <Grid Background="White">
        <local2:LogList HorizontalAlignment="Center" VerticalAlignment="Center" Width="auto" Height="200"/>
        <Button Content="Go Back" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" BorderThickness="0" Command="NavigationCommands.BrowseBack"/>
    </Grid>
</Page>
